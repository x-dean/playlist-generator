{
  "dashboard": {
    "id": null,
    "title": "Playlist Generator Dashboard",
    "tags": ["playlist-generator"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Track Analysis Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "playlista_track_analysis_total",
            "legendFormat": "{{status}} - {{format}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Analysis Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(playlista_analysis_duration_seconds_sum[5m]) / rate(playlista_analysis_duration_seconds_count[5m])",
            "legendFormat": "{{format}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Database Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "playlista_tracks_total",
            "legendFormat": "Total Tracks"
          },
          {
            "expr": "playlista_playlists_total",
            "legendFormat": "Total Playlists"
          },
          {
            "expr": "playlista_analysis_results_total",
            "legendFormat": "Analysis Results"
          }
        ]
      },
      {
        "id": 4,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "playlista_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          },
          {
            "expr": "playlista_cpu_usage_percent",
            "legendFormat": "CPU Usage (%)"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
} 